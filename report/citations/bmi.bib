	Journal = {Cerebral Cortex},
	Number = {11},
	Pages = {1246-1255},
	Title = {Fine Modulation in Network Activation during Motor Execution and Motor Imagery},
	Url = {http://cercor.oxfordjournals.org/content/14/11/1246.abstract},
